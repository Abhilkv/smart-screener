{
  "name": "dms-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "link": "react-native link",
    "clean": "cd android && ./gradlew clean",
    "start-dev-android": "react-native run-android --variant=devDebug --appIdSuffix=dev",
    "start-prd-android": "react-native run-android --variant=productionDebug --appIdSuffix=production",
    "start-stg-android": "react-native run-android --variant=stagingDebug --appIdSuffix=staging",
    "build-dev": "cd android && ./gradlew assembleDevRelease",
    "build-stg": "cd android && ./gradlew assembleStagingRelease",
    "build-prd": "cd android && ./gradlew assembleProductionRelease",
    "bundle-app": "cd android && ./gradlew bundleRelease",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
